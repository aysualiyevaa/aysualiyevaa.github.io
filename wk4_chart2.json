{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Renewables",
      "anchor": "start",
      "subtitle": "Output, GWh, | IEA via ECO-API",
      "fontSize": "14",
      "subtitleFontSize": "12",
      "color": "#ed108a",
      "subtitleColor": "#f102a1"
    },
    "height": "250",
    "width": "250",
    "autosize": {
      "type": "fit",
      "contains": "padding"
    },
    "background": "#ffffff",
    "view": {
      "fill": "#ffffff",
      "strokeOpacity": 0
    },
    "data": {
      "url": "https://api.economicsobservatory.com/gbr/rGWh?vega",
      "format": {
        "type": "json"
      }
    },
    "mark": {
      "type": "line",
      "color": "#e00aa7",
      "strokeWidth": "2"
    },
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {
          "title": "",
          "titleColor": "#e513e8",
          "grid": false
        }
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      }
    }
  }